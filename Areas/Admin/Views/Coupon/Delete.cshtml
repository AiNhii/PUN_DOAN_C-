@model comestic_csharp.Models.Coupon

@{

    ViewData["Title"] = "Delete";
}

<div class="pcoded-content">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Are you sure you want to delete this?</h5>
            <h4>Coupon</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Code)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Code)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Type)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Type)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Value)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Value)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Status)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Status)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.IsVoucher)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.IsVoucher)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Quantity)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Quantity)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.StartedAt)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.StartedAt)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.EndedAt)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.EndedAt)
                </dd>
            </dl>
        </div>
        <div class="card-footer">
            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Delete" class="btn btn-danger" /> |
                <a asp-action="Index" class="btn btn-primary">Back to List</a>
            </form>
        </div>
    </div>
</div>


